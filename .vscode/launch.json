{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Debug Cucumber Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@cucumber/cucumber/bin/cucumber.js",
      "args": ["--format", "@cucumber/pretty-formatter"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "DEBUG_STEP_BY_STEP": "false"
      },
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ]
    },
    {
      "name": "Debug Cucumber Tests - Step by Step",
